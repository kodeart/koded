{
  "name": "koded/framework",
  "version": "1.0.0",
  "type": "library",
  "license": "BSD-3-Clause",
  "description": "Micro framework for ReST apps",
  "keywords": [
    "koded",
    "micro",
    "framework"
  ],
  "require": {
    "php": "^8",
    "psr/http-server-middleware": "^1",
    "koded/container": "^2",
    "koded/cache-simple": "^3",
    "koded/http": "^3",
    "koded/logging": "^3",
    "koded/session": "2.*",
    "filp/whoops": "^2.7",
    "ext-zlib": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-gettext": "*"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "exclude-from-classmap": [
      "docs/",
      "locales/",
      "public/",
      "tests/"
    ],
    "files": [
      "src/Error/errors.php",
      "src/Support/semver.php",
      "src/Support/responders.php",
      "src/Support/functions.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "symfony/var-dumper": "^5",
    "twig/twig": "~1"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Koded\\Framework\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}